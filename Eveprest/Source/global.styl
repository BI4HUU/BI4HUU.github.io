body
	// background #ddd
	margin 0
	padding 0
	color #777

@font-face 
	font-family 'fa'
	src url('font/fontawesome-webfont.woff2')
	font-weight normal
	font-style normal
@font-face 
	font-family 'fl'
	src url('font/fl-chapps.woff')
	font-weight normal
	font-style normal
.fa
	font-family 'fa'
	-webkit-font-smoothing antialiased
	display inline-block
	font-style normal
	font-variant normal
	text-rendering auto
	line-height 1
.fl:after
	font-family 'fl'
	font-size 1.125rem
	font-weight 400
.fl
	font-family 'fl'
	-webkit-font-smoothing antialiased
	display inline-block
	font-style normal
	font-variant normal
	text-rendering auto
	line-height 1
